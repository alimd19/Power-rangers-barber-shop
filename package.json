  "main": "index.js",
  "scripts": {
    "setup": "npm i && cd backend && npm i && cd ../frontend && npm i",
    "frontend": "cd frontend && npm start",
    "backend": "cd backend && nodemon src/index.js",
    "dev": "concurrently --kill-others-on-fail \"npm run backend\" \"npm run frontend\"",
